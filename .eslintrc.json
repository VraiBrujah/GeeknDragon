{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "airbnb-base",
  "parserOptions": {
    "ecmaVersion": 2021
  },
  "rules": {
    "linebreak-style": ["error", "unix"],
    "indent": ["error", 4],
    "no-console": "off",
    "camelcase": "off",
    "no-underscore-dangle": "off",
    "no-param-reassign": "off",
    "no-plusplus": "off",
    "max-len": ["error", { "code": 120 }]
  }
}
